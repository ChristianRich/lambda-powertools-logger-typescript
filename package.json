    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.3.7",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "serverless-esbuild": "1.33.0",
    "serverless-offline": "11.2.1",
    "serverless-prune-plugin": "1.5.0",
    "serverless-webpack": "5.2.0",
    "ts-loader": "8.0.10",
    "ts-node": "9.1.1",
    "tsconfig-paths-webpack-plugin": "3.3.0",
    "typescript": "4.0.5"
